<%= render "filenotes/header" %>
<script type="text/javascript" src="jquery-3.3.1.min.js"></script>
<div class='main-bar'>
  <p class='note'>>>Works</p>
  <%=link_to image_tag("plus.png"), new_work_path, class: 'plus' %>
</div>
<div class='head-border'>
</div>

<div class='work-contents'>
  <% @works.reverse_each do |work| %>
    <div class='work-content'><%= work.app_name %></div>
  <% end %>
</div>